<div class="card-header pt-3 pb-3 text-center bg-primary">
    {% if admin_theme_logo() %}
        <img src="{{ asset(admin_theme_logo()) }}">
    {% else %}
        <h4>{% if admin_theme_icon() %}{{ (admin_theme_icon() ~ ' me-1') | icon }}{% endif %} {{ admin_theme_name() }}</h4>
    {% endif %}
</div>