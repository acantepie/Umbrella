{
    "name": "umbrella/adminbundle",
    "type": "symfony-bundle",
    "description": "Umbrella Admin Symfony bundle.",
    "keywords": [
        "backend",
        "admin",
        "umbrella"
    ],
    "homepage": "https://github.com/acantepie/umbrella",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrien Cantepie",
            "email": "adrien.cantepie@gmail.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": ">=7.4",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/orm": "^2.8",
        "symfony/asset": "^5.4|^6.0",
        "symfony/console": "^5.4|^6.0",
        "symfony/config": "^5.4|^6.0",
        "symfony/dependency-injection": "^5.4|^6.0",
        "symfony/filesystem": "^5.4|^6.0",
        "symfony/form": "^5.4|^6.0",
        "symfony/framework-bundle": "^5.4|^6.0",
        "symfony/http-foundation": "^5.4|^6.0",
        "symfony/http-kernel": "^5.4|^6.0",
        "symfony/mailer": "^5.4|^6.0",
        "symfony/mime": "^5.4|^6.0",
        "symfony/routing": "^5.4|^6.0",
        "symfony/security-bundle": "^5.4|^6.0",
        "symfony/translation": "^5.4|^6.0",
        "symfony/options-resolver": "^5.4|^6.0",
        "symfony/twig-bundle": "^5.4|^6.0",
        "symfony/yaml": "^5.4|^6.0",
        "umbrella2/corebundle": "*",
        "symfony/maker-bundle": "^1.21"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "symfony/browser-kit": "^5.4|^6.0",
        "symfony/css-selector": "^5.4|^6.0",
        "symfony/phpunit-bridge": "^5.4|^6.0"
    },
    "suggest": {
        "knplabs/knp-time-bundle": "Display pretty time on notification"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Umbrella\\AdminBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Umbrella\\AdminBundle\\Tests\\": "tests/",
            "Umbrella\\AdminBundle\\Tests\\TestApp\\": "tests/Functional/app/src/"
        }
    }
}
